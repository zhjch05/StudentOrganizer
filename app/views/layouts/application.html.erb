<!DOCTYPE html>
<html>
<head>
  <title><%= content_for(:page_title) || "Student Organizer" %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render "nav_top" %>
<div class="container">
    <div class="col-xs-12">
        <%= render "breadcrumbs" %>
    </div>
    <div class="col-xs-3">
        <%= render "datepicker" %>
    </div>
    <div class="col-md-9">
        <%= yield %>
    </div>

</div>

</body>
</html>
